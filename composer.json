{
    "name": "laravel-liberu/tasks",
    "description": "Task management dependency for Laravel Liberu",
    "keywords": [
        "laravel-liberu",
        "tasks",
        "task-management"
    ],
    "homepage": "https://github.com/laravel-liberu/tasks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        },
        {
            "name": "Reza Fouladi",
            "email": "raftx24@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/dynamic-methods": "^2.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Tasks\\": "src/",
            "LaravelLiberu\\Tasks\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Tasks\\AppServiceProvider",
                "LaravelLiberu\\Tasks\\AuthServiceProvider",
                "LaravelLiberu\\Tasks\\BroadcastServiceProvider",
                "LaravelLiberu\\Tasks\\WebsocketServiceProvider",
                "LaravelLiberu\\Tasks\\EnumServiceProvider"
            ]
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
